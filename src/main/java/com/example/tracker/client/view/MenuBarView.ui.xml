<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <g:HorizontalPanel ui:field="horizontalPanel" width="100%">
            <g:Label ui:field="usernameLabel"/>
            <g:Button ui:field="manageProfilesButton" text="Manage Profiles"/>
            <g:Button ui:field="manageTypesButton" text="Manage Types"/>
            <g:cell width="100%">
                <g:Button ui:field="logoutButton" text="Logout"/>
            </g:cell>
        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder>